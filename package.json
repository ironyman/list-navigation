{
  "name": "list-navigation",
	"displayName": "list-navigation",
	"description": "Move over groupings of text delimited by parentheses, brackets, braces, etc",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.61.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onCommand:list-navigation.forward-list",
        "onCommand:list-navigation.backward-list",
        "onCommand:list-navigation.backward-up-list",
        "onCommand:list-navigation.down-list",
        "onCommand:list-navigation.expand-select-list",
        "onCommand:list-navigation.helloWorld"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "list-navigation.forward-list",
				"title": "Hello World"
			},
			{
				"command": "list-navigation.backward-list",
				"title": "Hello World"
			},
			{
				"command": "list-navigation.backward-up-list",
				"title": "Hello World"
			},
			{
				"command": "list-navigation.down-list",
				"title": "Hello World"
			},
			{
				"command": "list-navigation.expand-select-list",
				"title": "Hello World"
			},
			{
				"command": "list-navigation.helloWorld",
				"title": "Hello World"
			}
		],
		"keybindings": [
			{
				"command": "list-navigation.forward-list",
				"key": "ctrl+alt+n",
				"mac": "cmd+alt+n"
			},
			{
				"command": "list-navigation.backward-list",
				"key": "ctrl+alt+p",
				"mac": "cmd+alt+p"
			},
			{
				"command": "list-navigation.backward-up-list",
				"key": "ctrl+alt+u",
				"mac": "cmd+alt+u"
			},
			{
				"command": "list-navigation.down-list",
				"key": "ctrl+alt+d",
				"mac": "cmd+alt+d"
			},
			{
				"command": "list-navigation.expand-select-list",
				"key": "ctrl+alt+e",
				"mac": "cmd+alt+e"
			},
			{
				"command": "list-navigation.helloWorld",
				"key": "ctrl+alt+h",
				"mac": "cmd+alt+h"
			}
        ]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.65.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.12.1",
		"@typescript-eslint/parser": "^5.12.1",
		"eslint": "^8.9.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.1",
		"typescript": "^4.5.5",
		"ts-loader": "^9.2.6",
		"webpack": "^5.69.1",
		"webpack-cli": "^4.9.2",
		"@vscode/test-electron": "^2.1.2"
	}
}
